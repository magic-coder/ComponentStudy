<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@id/root_id"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:background="@drawable/detail_child_node_bg">

    <View style="@style/ItemSeperatorLayout" />


    <TextView
        android:id="@id/location"
        style="@style/TableChildHeaderStyle"
        android:layout_width="120dp"
        android:text="@string/location" />

    <View style="@style/ItemSeperatorLayout" />


    <TextView
        android:id="@id/batchFlag"
        style="@style/TableChildHeaderStyle"
        android:layout_width="120dp"
        android:text="@string/batch_flag" />

    <View style="@style/ItemSeperatorLayout" />


    <TextView
        android:id="@id/quantity"
        style="@style/TableChildHeaderStyle"
        android:layout_width="120dp"
        android:text="@string/delivery_quantity" />

    <View style="@style/ItemSeperatorLayout" />


    <TextView
        android:id="@id/cqyt_tv_quantity_custom"
        style="@style/TableChildHeaderStyle"
        android:layout_width="120dp"
        android:text="@string/cqyt_quantity_custom" />

    <View style="@style/ItemSeperatorLayout" />


    <TextView
        android:id="@id/cqyt_tv_location_type"
        style="@style/TableChildHeaderStyle"
        android:layout_width="120dp"
        android:text="@string/location_type" />

    <View style="@style/ItemSeperatorLayout" />
</LinearLayout>
